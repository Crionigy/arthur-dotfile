{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "final_space": false,
    "osc99": true,
    "blocks": [{
        "type": "prompt",
        "alignment": "left",
        "segments": [{
          "type": "session",
          "style": "diamond",
          "leading_diamond": "╭─> ",
          "trailing_diamond": "",
          "foreground": "#383837",
          "background": "#FFB6C1",
          "template": " {{ .UserName }} "
      }, {
            "type": "root",
            "style": "diamond",
            "leading_diamond": "",
            "trailing_diamond": "",
            "foreground": "#383837",
            "background": "#ef5350",
            "properties": {
                "root_icon": "\uf292",
                "prefix": "<parentBackground>\ue0b0</> "
            }
        }, {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#e4e4e4",
          "background": "#444444",
          "template": " \uf74a {{ .Path }} ",
          "properties": {
              "style": "folder",
              "enable_hyperlink": true,
              "home_icon": "~"
          }
        }, {
            "type": "git",
            "style": "powerline",
            "powerline_symbol": "\ue0b0",
            "foreground": "#e4e4e4",
            "background": "#161b22",
            "background_templates": [
                "{{ if or(.Working.Changed) (.Staging.Changed) }}#ffeb95{{ end }}",
                "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#c5e478{{ end }}",
                "{{ if gt .Ahead 0 }}#c792ea{{ end }}",
                "{{ if gt .Behind 0 }}#c792ea{{ end }}"
            ],
            "properties": {
                "branch_icon": "\ue725 ",
                "fetch_status": true,
                "fetch_upstream_icon": true,
                "template": "{{ .HEAD }} {{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#ef5350> \uF046 {{ .Staging.String }}</>{{ end }}"
            }
        }]
    }, {
        "type": "prompt",
        "alignment": "right",
        "segments": [{
            "type": "node",
            "style": "diamond",
            "leading_diamond": " \uE0B6",
            "trailing_diamond": "\uE0B4",
            "foreground": "#383837",
            "background": "#5EB160",
            "properties": {
              "prefix": "\uE718 ",
              "postfix": "",
              "display_package_manager": true,
              "yarn_icon": " <#348cba></>",
              "npm_icon": " <#cc3a3a></> "
              }
          }, {
            "type": "java",
            "style": "diamond",
            "leading_diamond": " \uE0B6",
            "trailing_diamond": "\uE0B4",
            "foreground": "#383837",
            "background": "#B02321",
            "properties": {
              "prefix": "\uE718 ",
              "postfix": "",
              "display_package_manager": true,
              "yarn_icon": " <#348cba></>",
              "npm_icon": " <#cc3a3a></> "
              }
          }, {
              "type": "time",
              "style": "diamond",
              "invert_powerline": true,
              "leading_diamond": " \uE0B6",
              "trailing_diamond": "\uE0B4",
              "background": "#FFB6C1",
              "foreground": "#383837",
              "properties": {
                  "prefix": " \uf5ef ",
                  "postfix": " "
              }
          }]
      }, {
          "type": "prompt",
          "alignment": "left",
          "newline": true,
          "segments": [{
            "foreground": "#FFB6C1",
            "properties": {
              "always_enabled": true
            },
            "style": "diamond",
            "template": "╰─> ",
            "type": "exit"
          }]
      }],
    "version": 2
  }
  